{
  "manifest_version": 2,
  "name": "NinjaMaps",
  "description": "Navigation tool for ninjas.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      // "css": ["mystyles.css"],
      "js": ["jquery.js", "ninja.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://maps.google.com/",
    "https://ajax.googleapis.com/"
  ]
}